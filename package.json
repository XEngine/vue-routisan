{
  "name": "vue-routisan",
  "description": "Elegant, fluent route definitions for Vue Router, inspired by Laravel.",
  "version": "3.0.0-alpha.1",
  "license": "ISC",
  "author": "@mikerockett",
  "homepage": "https://vue-routisan.rockett.pw",
  "keywords": [
    "vue",
    "vuejs",
    "vue-router",
    "laravel",
    "guard",
    "middleware",
    "artisan"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mikerockett/vue-routisan.git"
  },
  "bugs": {
    "url": "https://github.com/mikerockett/vue-routisan/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "microbundle": "^0.12.3",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "vue-router": "^3.0.1"
  },
  "source": "src/main.js",
  "main": "dist/vue-routisan.js",
  "mangle": {
    "regex": "^_"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "bundle": "microbundle --strict --no-sourcemap",
    "watch": "npm run clean && microbundle watch --no-compress --strict --format umd",
    "build": "npm run clean && npm run bundle",
    "prepublishOnly": "npm run build"
  }
}
